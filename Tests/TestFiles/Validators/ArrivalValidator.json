{
  "eventType": "Arrival",
  "title": "Arrival Event JSON Schema Validator",
  "properties": [
    {
      "name" :  "eventType",
      "type": "string",
      "validationRegex": "",
      "errorMessage": "Invalid event type",
      "isRequired": "true"
    },
    {
      "name": "timeStamp",
      "type": "string",
      "validationRegex": "^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9]) (2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])?(Z)?$",
      "errorMessage": "Invalid time stamp",
      "isRequired": "true"
    },
    {
      "name": "flight",
      "type": "string",
      "validationRegex": "^[a-zA-Z0-9]{5,6}$",
      "errorMessage": "Invalid flight number",
      "isRequired": "true"
    },
    {
      "name": "delayed",
      "type": "string",
      "validationRegex": "^[0-9][0-9]:[0-5][0-9]$",
      "errorMessage": "Invalid delayed time",
      "isRequired": "true"
    },
    {
      "name": "passengers",
      "type": "integer",
      "validationRegex": "^\\d+$",
      "errorMessage": "Invalid passengers field",
      "isRequired": "true"
    }
  ]
}
